        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handle various in-game variables

//Set what object is being followed
event_user(1);

//If the player does exist
if (follow != noone)
&amp;&amp; (instance_exists(follow)) {
    
    //Make sure the barrier is not active
    if (barrier == 0) {
         
        //Prevent player from going outside
        event_user(0);
        
        //Check where the player is
        playerx = max(playerx, follow.x-pos);
        
        //Update pos
        if ((follow.hspeed &gt;= 0) 
        &amp;&amp; (follow.x &gt; x-xprevious))
        &amp;&amp; (follow.x &gt; view_wview div 2 - 48) {
        
            //Update only if the player is moving right or it's at the right
            if (follow.hspeed &gt; 0)
            || (follow.x &gt; x-xprevious) {
                
                pos += abs(follow.hspeed)/2;
                if (pos &gt; 56)
                    pos = 56;
            }
        }
        
        //Reset pos
        if (follow.x &lt; view_xview[view_current]+64)
        &amp;&amp; (pos &gt; 0) {
        
            leftedge -= pos;
            pos = 0;
        }
    }
    
    //Otherwise, force set position of the camera
    else if (barrier == 1) {
    
        //Check where the player is
        playerx = max(playerx, follow.x);
    }
    
    //Update the left edge
    leftedge = playerx - padding;  
    
    //Update view_xview
    view_xview[view_current] = leftedge;
    if (view_xview[view_current] &lt; 0)
        view_xview[view_current] = 0;
    if (view_xview[view_current] &gt; room_width-global.gw)
        view_xview[view_current] = room_width-global.gw;
}

//Round position variables
view_xview[view_current] = round(view_xview[view_current]);

//Update parallax background position
for (var i=0; i&lt;8; i++) {

    xx[i] = view_xview/(1.2+(0.4*i));
}

//Deactivate enemies outside the view
instance_deactivate_object(obj_deactenemyparent);
instance_deactivate_object(obj_deactivateparent);

//Activate deactivated instances inside the activation region
instance_activate_region(view_xview[view_current] - 32, view_yview - 32, view_wview[view_current] + 32, view_hview[view_current] + 32, true);
</string>
