          <argument>
            <kind>1</kind>
            <string>///Lakitu logic

//If the player dies, stop everything
if (instance_exists(obj_player_dead)) {

    scr_freeze();
    exit;
}

//Select an object to follow.
if (instance_exists(obj_playerparent))
    follow = obj_playerparent;
else if (instance_exists(obj_levelcontrol))
    follow = obj_levelcontrol;
else {

    follow = noone;
    hspeed = 0;
}

//If there's an object to follow.
if (follow != noone) {

    //If the object to follow is inside the spawn area.
    if (follow.x &gt; xmin)
    &amp;&amp; (follow.x &lt; xmax) {
    
        //If the object to follow is at the right, move to the right.    
        if (follow.x &gt; x) {
        
            hspeed += 0.0324;
            if (hspeed &gt; 2)
                hspeed = 2;
        }
        
        //Otherwise, move to the left.
        else if (follow.x &lt; x) {
        
            hspeed += -0.0324;
            if (hspeed &lt; -2)
                hspeed = -2;
        }
        
        //Prevent it from exiting the view
        if (x &lt; view_xview[0]-16)
            x = view_xview[0]-16;
        else if (x &gt; view_xview[0]+view_wview[0]+16)
            x = view_xview[0]+view_wview[0]+16;
    }
    
    //Otherwise if it's outside, move outside the view.
    else {
    
        //Move outside
        if (follow.x &gt; (xmax-xmin)/2+xmin) {
            
            hspeed += 0.0324;
            if (hspeed &gt; 2)
                hspeed = 2;
        }
        else if (follow.x &lt; (xmax-xmin)/2+xmin) {
            
            hspeed += -0.0324;
            if (hspeed &lt; -2)
                hspeed = -2;            
        }
        
        //Keep outside
        inside = false;
    }
}

//Set the scale
if (hspeed &gt; 0)
    xscale = 1;
else
    xscale = -1;
    
//Stay in sight
y = view_yview[0]+48;
    
//Snap outside the level when outside the view
if (x &lt; view_xview[0]-32)
    x = -32;
if (x &gt; view_xview[0]+view_wview[0]+32)
    x = room_width+32;
</string>
