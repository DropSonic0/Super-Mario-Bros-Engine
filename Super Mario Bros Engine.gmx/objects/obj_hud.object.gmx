    draw_text(view_xview[0]+152, view_yview[0]+24, string_upper(global.world_m));
    if (global.hardmode == 1)
        draw_text(view_xview[0]+144, view_yview[0]+24, string("+"));
}

//Luigi
else if (global.player == 1) {
        
    //Score
    draw_text(view_xview[0]+24, view_yview[0]+24, string_add_zeros(global.score_l, 6));
    
    //Coins
    draw_text(view_xview[0]+104, view_yview[0]+24, string_add_zeros(global.coins_l, 2));
    
    //World / Level
    draw_text(view_xview[0]+152, view_yview[0]+24, string(global.world_l));
    if (global.hardmode == 1)
        draw_text(view_xview[0]+144, view_yview[0]+24, string("+"));           
}

//Time
if (obj_levelcontrol.leveltime &gt; 0) {

    if (global.time &gt; 0)
